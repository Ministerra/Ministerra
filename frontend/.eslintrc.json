{
	"env": {
		"browser": true,
		"es2022": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended", "plugin:react-hooks/recommended", "plugin:react/jsx-runtime"],
	"rules": {
		"linebreak-style": "off",
		"no-mixed-spaces-and-tabs": "off",
		"quotes": "off",
		"indent": "off",
		"react-hooks/exhaustive-deps": "off",
		"react/prop-types": "off"
	},
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"settings": {
		"react": {
			"version": "detect"
		}
	}
}
